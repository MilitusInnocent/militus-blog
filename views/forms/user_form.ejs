<% if (title == 'Create User') { %>
  <div class="col-sm-12">    
    <div class="jumbotron">
      <%= title %> <br><br>
        <form action="" method="post">
          username:<br>
          <input type="text" name="username" placeholder="Username">
          <br>
          <input type="submit" value="Submit">
        </form>
    </div>
</div>
<% } else if (title == 'Update User') {%>
  <div class="col-sm-12">    
    <div class="jumbotron">
      <%= title %> <br><br>
        <form action="" method="post">
          username:<br>
          <input type="text" name="username" value= <%= user.username %>>
          <br>
          <input type="submit" value="Update">
        </form>
    </div>
</div>
<%} else {%>
  <div class="col-sm-12">    
    <div class="jumbotron">
      <h1><%= title %> <br><br></h1>
    </div>
</div>
<%} %>